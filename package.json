{
  "name": "auth-yelbolt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --parallel build:*",
    "start": "npm-run-all --parallel start:*",
    "lint": "npm-run-all --parallel lint:*",
    "ui-color-palette": "npm-run-all --parallel dev:ui-color-palette dev:auth-yelbolt-worker",
    "dev:ui-color-palette": "cd ui-color-palette && npm run dev",
    "build:ui-color-palette": "cd ui-color-palette && npm run build",
    "start:ui-color-palette": "cd ui-color-palette && npm run start",
    "lint:ui-color-palette": "cd ui-color-palette && npm run lint",
    "patch:ui-color-palette": "cd ui-color-palette && npm version patch",
    "minor:ui-color-palette": "cd ui-color-palette && npm version minor",
    "major:ui-color-palette": "cd ui-color-palette && npm version major",
    "ideas-spark-booth": "npm-run-all --parallel dev:ideas-spark-booth dev:auth-yelbolt-worker",
    "dev:ideas-spark-booth": "cd ideas-spark-booth && npm run dev",
    "build:ideas-spark-booth": "cd ideas-spark-booth && npm run build",
    "start:ideas-spark-booth": "cd ideas-spark-booth && npm run start",
    "lint:ideas-spark-booth": "cd ideas-spark-booth && npm run lint",
    "patch:ideas-spark-booth": "cd ideas-spark-booth && npm version patch",
    "minor:ideas-spark-booth": "cd ideas-spark-booth && npm version minor",
    "major:ideas-spark-booth": "cd ideas-spark-booth && npm version major",
    "dev:auth-yelbolt-worker": "cd ../auth-yelbolt-worker && npm run start:8787"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.1.0"
  }
}